{
  "name": "@aionx/source",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "pnpm@8.0.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "lint-staged": "lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "husky": {
      "hooks": {
        "prepare-commit-msg": "exec < /dev/tty && pnpm cz --hook || true"
      }
    }
  },
  "lint-staged": {
    "./packages/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "nx run-many --targets=lint --verbose"
    ]
  },
  "private": true,
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@radix-ui/react-icons": "^1.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.314.0",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@nx/cypress": "17.3.0-canary.20240120-06717de",
    "@nx/eslint": "17.3.0-canary.20240120-06717de",
    "@nx/eslint-plugin": "17.3.0-canary.20240120-06717de",
    "@nx/jest": "17.3.0-canary.20240120-06717de",
    "@nx/js": "17.3.0-canary.20240120-06717de",
    "@nx/next": "17.3.0-canary.20240120-06717de",
    "@nx/react": "17.3.0-canary.20240120-06717de",
    "@nx/workspace": "17.3.0-canary.20240120-06717de",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.4.1",
    "commitizen": "^4.3.0",
    "cypress": "^13.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.48.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.13.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^15.2.0",
    "nx": "17.2.8",
    "postcss": "8.4.21",
    "prettier": "^2.6.2",
    "prisma": "^5.8.1",
